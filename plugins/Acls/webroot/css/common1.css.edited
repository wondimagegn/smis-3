#submenu {
     width:100%;
   /* height:30px;*/
    height:25px;
    background-color:#0288D8;
    margin-bottom:3px;
    padding-top:3px;
    padding-bottom:3px;
}

#submenu ul li  {
        list-style:none; 
        padding: 0;
        font: bold 13px Verdana;
        position: relative;
        background-color:#0288D8;
        
}

#submenu ul.dropdown                { position: relative;margin: 0px;}
#submenu ul.dropdown li                      { font-weight: bold; float: left; zoom: 1; background: #0288D8; }
#submenu ul.dropdown a:hover		            { color: #000; }
#submenu ul.dropdown a:active                { color: #0288D8; }
#submenu ul.dropdown li a  { 
display: block;  
border-right: 1px solid #333;
color: white; 

}
#submenu ul.dropdown li:last-child a         { border-right: none; } /* Doesn't work in IE */
#submenu ul.dropdown li.hover,
#submenu ul.dropdown li:hover                { background: #0288D8; color: black; position: relative; }
#submenu ul.dropdown li.hover a              { color: white; }


/* 
	LEVEL TWO
*/
#submenu ul.dropdown ul 						{ width: 220px; visibility: hidden; position: absolute; top: 100%; left: -30px; }
#submenu ul.dropdown ul li 	{ font-weight: normal; background: #0288D8; color: #000;
							  border-bottom: 1px solid #F0F0F0; float: none; padding-top: 3px; padding-bottom: 3px;
							}
									  
                                    /* IE 6 & 7 Needs Inline Block */
/* Internal menu format*/
#submenu ul.dropdown ul li a					{ border-right: none; width: 100%; display: inline-block } 
#submenu ul.dropdown ul li a:link	{ color:#fff } 
#submenu ul.dropdown ul li a:active	{ color:#fff } 
#submenu ul.dropdown ul li a:hover	{ color:#000 } 
#submenu ul.dropdown ul li a:visited	{ color:#fff } 
